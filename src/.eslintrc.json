{     
    "parser": "babel-eslint",  
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true
    },
    "globals": {
        "$": true,
        "_": true
    },
    "extends": "airbnb-base",    
    "rules": {
        "import/no-unresolved": "off"
    }
}


